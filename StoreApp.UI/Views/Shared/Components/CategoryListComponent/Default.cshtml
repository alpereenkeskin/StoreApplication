@model List<Category>
<table class="table">
    <h4 class="text-muted"> Kategoriler</h4>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td><a class="btn btn-outline-dark w-100" href="#">@item.CategoryName</a></td>
            </tr>
        }

    </tbody>
</table>