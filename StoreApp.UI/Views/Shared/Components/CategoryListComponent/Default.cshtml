@model List<Category>
<table class="table">
    <h4 class="text-muted"> Kategoriler</h4>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td><a asp-action="Index" asp-controller="Product" asp-route-categoryId="@item.CategoryId"
                        class="btn btn-outline-dark w-100">@item.CategoryName</a></td>
            </tr>
        }

    </tbody>
</table>