@model List<Order>
@{
    Layout = "_AdminLayout";
}

<h1>Order Manager</h1>

<div>
    
    @if (Model.Count() > 0)
    {
        @foreach (Order order in Model)
        {
            <partial name="_OrderPartial" model="@order"></partial>
        }
    }
    else
    {
        <div class="alert alert-info">
            <p class="lead">There is any order yet.</p>
        </div>
    }

</div>