@model IEnumerable<IdentityRole>
@{
    Layout = "_AdminLayout";
}
<div class="container">
    <h1>Roles</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">ID</th>
                <th scope="col">Roles</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var role in Model)
            {
                <tr>
                    <th scope="row">#</th>
                    <td>@role.Id</td>
                    <td>@role.Name</td>
                </tr>

            }
        </tbody>
    </table>



</div>